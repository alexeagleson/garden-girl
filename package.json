    "cross-env": "^7.0.0",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "schemats": "^3.0.3",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^6.0.0",
    "typescript": "^3.8.3"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  }
}
