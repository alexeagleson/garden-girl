    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "cron-parser": "^2.13.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "immutable": "^4.0.0-rc.12",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.2.2",
    "nodemon": "^2.0.2",
    "pg": "^7.12.1",
    "pixi.js": "^5.2.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-icons": "^3.8.0",
    "react-is": "^16.13.0",
